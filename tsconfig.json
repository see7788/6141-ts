{
  "ts-node": { "esm": true },
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "paths": {
      "@uipublic/*": ["./src/pcbDz002/cppWeb/public/*"],
      "@ui/*": ["./src/pcbDz002/cppWeb/protected/*"],
    },
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src/"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}